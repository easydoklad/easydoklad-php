{
    "name": "easydoklad/easydoklad-php",
    "description": "PHP SDK pre easyDoklad",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Stovka",
            "email": "ps@stacktrace.sk"
        }
    ],
    "require": {
        "php": "^8.2",
        "saloonphp/pagination-plugin": "^2.0",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "pestphp/pest": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "EasyDoklad\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyDoklad\\SDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
